  },
  "author": "Robin Bakker",
  "license": "MIT",
  "keywords": [],
  "private": true,
  "dependencies": {
    "clean-css": "^5.2.4",
    "cssnano": "^5.1.5",
    "dotenv": "^16.0.0",
    "eleventy-plugin-pwa": "^1.0.8",
    "html-minifier": "^4.0.0",
    "markdown-it-link-attributes": "^4.0.0",
    "terser": "^5.12.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^2.0.0",
    "cross-env": "^7.0.3",
    "del-cli": "3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12"
  },
  "prettier": {
